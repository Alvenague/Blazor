@using Microsoft.Extensions.Logging;
@inject ILogger<TestLogger> logger
@page "/"

<h1>Counter</h1>

<button class="btn btn-primary" id="testButton" onclick="@ButtonClick">Click me</button>

@functions {

    protected override async Task OnInitAsync()
    {
        logger.LogDebug("OnInitAsync");
    }

    void ButtonClick()
    {
        logger.LogInformation("Button Click");
    }
}
